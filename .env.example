# OpenRouter API Configuration
OPENROUTER_API_KEY=API_KEY_HERE

# Default Models (Fast and Reliable)
MODEL_A=deepseek/deepseek-chat-v3-0324:free
MODEL_B=qwen/qwen3-coder:free

# Judge Model (Fast, Non-Reasoning for Speed)
JUDGE_MODEL=google/gemini-2.0-flash-exp:free

# Performance Settings
JUDGE_REPEATS=1
MAX_TOKENS=512
TEMPERATURE=0.1
TOP_P=0.95

# Timeout Settings (Aggressive for Free Models)
HTTP_TIMEOUT=60

# UI Performance Settings
DEFAULT_PERFORMANCE_MODE=Balanced
ENABLE_STREAMING=true
SHOW_PROGRESS_ESTIMATES=true